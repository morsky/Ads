@model IEnumerable<Ads.ViewModels.ApplicationUserViewModel>
@{
    ViewBag.Title = "ListUsers";
}

<h2>List Users</h2>

User count: @Model.Count()

@foreach (var user in Model)
{
    <p>@user.UserName</p>
    <p>
        @Html.ActionLink("Edit User Role", "EditUser", "Admin", new { id = user.Id }, null) |
        @Html.ActionLink("Delete User", "DeleteUser", "Admin", new { id = user.Id }, null)
    </p>
}